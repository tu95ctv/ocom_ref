<?xml version="1.0"?>
<odoo>
    <data noupdate="1">

		<record model="ir.config_parameter" id="aeroo_conversion_command" >
            <field name="key">aeroo.conversion_command</field>
            <field name="value">soffice</field>
        </record>
        <record model="ir.config_parameter" id="aeroo_conversion_generate" >
            <field name="key">aeroo.conversion_generate</field>
            <field name="value">py3o</field>
        </record>

	    <record model="report.mimetypes" id="report_mimetypes_odt_odt" >
		    <field name="name">ODF Text Document (.odt)</field>
		    <field name="code">oo-odt</field>
		    <field name="compatible_types">oo-odt</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_ods_ods" >
		    <field name="name">ODF Spreadsheet (.ods)</field>
		    <field name="code">oo-ods</field>
		    <field name="compatible_types">oo-ods</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_raw" >
		    <field name="name">Generic</field>
		    <field name="code">genshi-raw</field>
		    <field name="compatible_types">genshi-raw</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_pdf_odt" >
		    <field name="name">PDF - Portable Document Format (.pdf)</field>
		    <field name="code">oo-pdf</field>
		    <field name="compatible_types">oo-odt</field>
		    <field name="filter_name">writer_pdf_Export</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_pdf_ods" >
		    <field name="name">PDF - Portable Document Format (.pdf)</field>
		    <field name="code">oo-pdf</field>
		    <field name="compatible_types">oo-ods</field>
		    <field name="filter_name">calc_pdf_Export</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_doc_odt" >
		    <field name="name">Microsoft Word 97/2000/XP (.doc)</field>
		    <field name="code">oo-doc</field>
		    <field name="compatible_types">oo-odt</field>
		    <field name="filter_name">MS Word 97</field>
	    </record>

	    <record model="report.mimetypes" id="report_mimetypes_xls_odt" >
		    <field name="name">Microsoft Excel 97/2000/XP (.xls)</field>
		    <field name="code">oo-xls</field>
		    <field name="compatible_types">oo-ods</field>
		    <field name="filter_name">MS Excel 97</field>
	    </record>

		<record model="report.mimetypes" id="report_mimetypes_docx_odt" >
            <field name="name">Microsoft Word 2007 (.docx)</field>
            <field name="code">oo-docx</field>
            <field name="compatible_types">oo-odt</field>
            <field name="filter_name">MS Word 2007</field>
        </record>
        <record model="report.mimetypes" id="report_mimetypes_xlsx_odt" >
            <field name="name">Microsoft Excel 2007 (.xlsx)</field>
            <field name="code">oo-xlsx</field>
            <field name="compatible_types">oo-ods</field>
            <field name="filter_name">MS Excel 2007</field>
        </record>

	    <record model="report.mimetypes" id="report_mimetypes_csv_ods" >
		    <field name="name">Text CSV (.csv)</field>
		    <field name="code">oo-csv</field>
		    <field name="compatible_types">oo-ods</field>
		    <field name="filter_name">Text - txt - csv (StarCalc)</field>
	    </record>
    </data>
</odoo>


<odoo>
	<data>
			 <!-- QUAN -->
        <record id="quan_form" model="ir.ui.view">
            <field name="name">bds</field>
            <field name="model">res.country.district</field>
            <field name="inherit_id" ref="partner_vn_localization.as_vn_address_district_form_view"/>
            <field name="arch" type="xml">
                <form>
                    <field name="code" position="after">
                        <field name="name" />
                        <field name="state_id" />
                        <field name="name_viet_tat" />
                        <field name="muc_gia_quan" />
                        <field name="don_gia_dat_con_lai" />
                        <field name="post_ids" />
                    </field>
                
                </form>
            </field>
        </record>
        <record id="quan_list" model="ir.ui.view">
            <field name="name">res.country.district</field>
            <field name="model">res.country.district</field>
            <field name="inherit_id" ref="partner_vn_localization.as_vn_address_district_tree_view"/>
            <field name="arch" type="xml">
                    <field name="state_id" position="after">
                        <field name="name_without_quan" />
                        <field name="level" />
                        <!-- <field name="state_id" /> -->
                        <field name="name_viet_tat" />
                        <field name="len_post_ids" />
                        <field name="len_post_fix_ti_le" />
                        <field name="muc_gia_quan" />
                        <field name="don_gia_dat_con_lai" />
                        <field name="don_gia_mat_tien" />
                        <field name="don_gia_hxt" />
                        <field name="don_gia_hxh" />
                        <field name="don_gia_hbg" />
                        <field name="don_gia_hxm" />
                        <field name="don_gia_mat_tien_tc" />
                        <field name="don_gia_hxt_tc" />
                        <field name="don_gia_hxh_tc" />
                        <field name="don_gia_hbg_tc" />
                        <field name="don_gia_hxm_tc" />
                    </field>
            </field>
        </record>
        <record model="ir.actions.act_window" id="quan_action">
            <field name="name">Quáº­n</field>
            <field name="res_model">res.country.district</field>
            <field name="view_mode">tree,form</field>
        </record>
        <!-- !QUAN -->
	</data>
</odoo>